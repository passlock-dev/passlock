{
  "name": "sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:errors": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --threshold error",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "ncu": "ncu --peer -x @passlock/client -x better-sqlite3 -x postcss-load-config",
    "ncu:save": "ncu --peer -x @passlock/client -x better-sqlite3 -x postcss-load-config -u",
    "upgrade:pnpm": "corepack use pnpm@latest"
  },
  "devDependencies": {
    "@lucia-auth/adapter-sqlite": "^3.0.1",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.81.0",
    "@passlock/client": "0.9.15",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@types/better-sqlite3": "^7.6.10",
    "@types/google-one-tap": "^1.2.6",
    "autoprefixer": "^10.4.19",
    "better-sqlite3": "~9.6.0",
    "dedent": "^1.5.3",
    "js-sha256": "^0.11.0",
    "lucia": "^3.2.0",
    "lucide-svelte": "^0.396.0",
    "mode-watcher": "^0.3.1",
    "node-check-updates": "^0.1.9",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.1.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.2",
    "sveltekit-superforms": "^2.15.1",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "valibot": "^0.35.0",
    "vite": "^5.3.1"
  },
  "type": "module",
  "pnpm": {
    "overrides": {
      "valibot": "$valibot",
      "@effect/schema": "0.68.3",
      "@effect/platform": "0.57.8"
    }
  },
  "overrides": {
    "valibot": "^0.35.0",
    "@effect/schema": "0.68.3",
    "@effect/platform": "0.57.8"
  },
  "packageManager": "pnpm@9.4.0+sha256.b6fd0bfda555e7e584ad7e56b30c68b01d5a04f9ee93989f4b93ca8473c49c74"
}
