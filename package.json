{
  "name": "sveltekit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:errors": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --threshold error",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "ncu": "ncu --peer -x @passlock/client -x better-sqlite3 -x postcss-load-config",
    "ncu:save": "ncu --peer -u -x @passlock/client -x better-sqlite3 -x postcss-load-config"
  },
  "devDependencies": {
    "@lucia-auth/adapter-sqlite": "^3.0.1",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.81.0",
    "@passlock/client": "0.9.15",
    "@sveltejs/adapter-auto": "^3.2.1",
    "@sveltejs/kit": "^2.5.10",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@types/better-sqlite3": "^7.6.10",
    "@types/google-one-tap": "^1.2.6",
    "autoprefixer": "^10.4.19",
    "better-sqlite3": "~9.6.0",
    "dedent": "^1.5.3",
    "js-sha256": "^0.11.0",
    "lucia": "^3.2.0",
    "lucide-svelte": "^0.383.0",
    "mode-watcher": "^0.3.0",
    "node-check-updates": "^0.1.9",
    "postcss": "^8.4.38",
    "postcss-load-config": "^5.1.0",
    "prettier": "^3.3.0",
    "prettier-plugin-svelte": "^3.2.3",
    "svelte": "^4.2.17",
    "svelte-check": "^3.8.0",
    "sveltekit-superforms": "^2.14.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "valibot": "^0.30.0",
    "vite": "^5.2.12"
  },
  "type": "module",
  "packageManager": "pnpm@8.15.6+sha256.01c01eeb990e379b31ef19c03e9d06a14afa5250b82e81303f88721c99ff2e6f"
